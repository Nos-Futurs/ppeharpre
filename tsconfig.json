{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "lib": ["DOM"],
    "module": "ESNext",
    "moduleResolution": "node",
    "types": ["astro/client"],
    "noEmit": true,
    "paths": {
      "@*": ["./*"]
    }
  }
}
